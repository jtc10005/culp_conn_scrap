{
  "name": "culpepper-genealogy-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for Culpepper genealogy scraper and website",
  "scripts": {
    "install:scraper": "cd scraper && npm install",
    "install:app": "cd application && npm install",
    "install:all": "npm run install:scraper && npm run install:app",
    "scrape": "cd scraper && npm run scrape",
    "scrape:test": "cd scraper && npm run scrape:test",
    "dev": "cd application && npm run dev",
    "build": "cd application && npm run build",
    "start": "cd application && npm start",
    "lint:scraper": "cd scraper && npm run lint",
    "lint:app": "cd application && npm run lint",
    "lint": "npm run lint:scraper && npm run lint:app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtc10005/culp_conn_scrap.git"
  },
  "author": "",
  "license": "UNLICENSED"
}
